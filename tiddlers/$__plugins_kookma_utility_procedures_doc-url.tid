\procedure doc-url(url)
<$let topic={{{ [<url>split[.html]last[]] }}}
      ourl= {{{[<topic>addprefix[https://tiddlywiki.com]]}}} >
<a href=<<ourl>> target=_blank><$text text={{{[<topic>removeprefix[#]] :and[decodeuricomponent[]] }}} /></a>
\end